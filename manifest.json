{
  "manifest_version": 2,
  "name": "WakaTimeTab",
  "version": "1.0.9",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "https://wakatime.com/api/v1/",
    "activeTab"
  ],
  "description": "for developers Wakatime Tab",
  "browser_action": {
    "default_title": "WakaTimeTab"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "csp": "script-src 'self' 'unsafe-inline'; object-src 'self'",
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
